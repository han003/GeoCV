@using System.Web.Optimization

@{
    ViewBag.Title = "Legg til teknisk profil";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section css{
    @Styles.Render("~/bundles/css/tekniskprofil")
}
@section scripts{
    @Scripts.Render("~/bundles/js/tekniskprofil")
}

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Legg til teknisk profil</h1>
        </div>
    </div>

    <div class="row">

        <div class="col-sm-8">
            <div class="form-horizontal">

                <div class="form-group">
                    <label for="ny-profil-navn-txt" class="col-lg-2 control-label">Navn</label>
                    <div class="col-lg-8">
                        <input type="text" class="form-control" id="ny-profil-navn-txt" autocomplete="off">
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-lg-8 col-lg-offset-2">
                        <button id="ny-profil-lagre-btn" type="button" class="btn btn-success" tabindex="-1">Legg til</button>
                    </div>
                </div>

            </div>
        </div>

    </div>
</div>
