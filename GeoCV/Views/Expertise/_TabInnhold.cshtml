<div id="filter-row" class="row col-lg-12">
    <div class="col-lg-6">
        <input id="@ViewData["Tab"]-filter" type="text" class="form-control table-filter" placeholder="Filter" autocomplete="off">
    </div>
</div>

<div class="row">
    <div class="col-lg-6">
        <div class="col-lg-12">
            <table id="@ViewData["Tab"]-alle-tabell" class="table element-table">
                <thead>
                    <tr>
                        <th>Alle @ViewData["Tab"]</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>

    <div class="col-lg-6">
        <div class="col-lg-12">
            <table id="@ViewData["Tab"]-bruker-tabell" class="table bruker-table">
                <thead>
                    <tr>
                        <th>Mine @ViewData["Tab"]</th>
                        <th></th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>
