@using System.Web.Optimization

@{
    ViewBag.Title = "Prosjekt";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section css{
    @Styles.Render("~/bundles/css/projects")
}
@section scripts{
    @Scripts.Render("~/bundles/js/projects")
}

<div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">
    <div class="panel panel-default">
        <div class="panel-heading" role="tab" id="headingOne">
            <h4 class="panel-title">
                <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                    Opprett nytt prosjekt
                </a>
            </h4>
        </div>

        <div id="collapseOne" class="panel-collapse collapse" role="tabpanel" aria-labelledby="headingOne">
            <div class="panel-body">

                <div class="form-horizontal">

                    <div class="form-group">
                        <div class="col-lg-12">
                            <label>Kunde</label>
                            <input type="text" class="form-control update-txt" id="a">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-lg-12">
                            <label>Prosjekt Navn</label>
                            <input type="text" class="form-control update-txt" id="b">
                        </div>
                    </div>

                    <div class="form-group">
                        <div class="col-lg-12">
                            <label>Beskrivelse</label>
                            <input type="text" class="form-control update-txt" id="c">
                        </div>
                    </div>

                    <button type="button" class="btn btn-primary">Opprett prosjekt</button>

                </div>

            </div>
        </div>
    </div>
</div>

